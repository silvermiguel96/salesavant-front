    "lodash.get": "^4.4.2",
    "serve": "^11.0.1",
    "vue": "2.6.10",
    "vue-apollo": "3.0.0",
    "vue-router": "3.1.3",
    "vuetify": "2.1.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-cypress": "^3.7.0",
