{
  "name": "SaleSavant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "cpassets": "cp logo.jpg ./dist/logo.jpg",
    "build": "vue-cli-service build && npm run cpassets",
    "deploy": "npm run build && npm run force-commit && npm run ebdeploy",
    "ebdeploy": "eb deploy proxy-server-development",
    "force-commit": "git add . && sleep 5 && git commit -m 'deployment' && sleep 5 && git push && sleep 5",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "start": "node index.js",
    "dev": "NODE_ENV=local VUE=true node index.js"
  },
  "dependencies": {
    "auth0-js": "^9.10.4",
    "connect-history-api-fallback": "^1.6.0",
